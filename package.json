{
  "name": "llm-chat-app",
  "version": "1.0.0",
  "description": "Multi-LLM Chat Application",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && electron . --ignore-certificate-errors",
    "cli": "npm run build && node dist/main.js --cli",
    "build": "tsc -p tsconfig.json && webpack --config webpack.config.cjs && tsc -p tsconfig.preload.json",
    "watch": "tsc -w -p tsconfig.json & webpack --watch --config webpack.config.cjs",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "electron": "^28.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.3.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.24.0",
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "openai": "^4.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0"
  }
}
